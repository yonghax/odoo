<openerp>
	<data>
		<record id="ir_cron_module_scheduler_run" model="ir.cron">
			<field name="name">Reconcile Bank Statement - Invoice</field>
			<field eval="True" name="active"/>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">1</field>
			<field name="interval_type">minutes</field>
			<field name="numbercall">1</field>
			<field name="nextcall" eval="(DateTime.now() + timedelta(minutes=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
			<field eval="False" name="doall"/>
			<field name="model">account.bank.statement.line</field>
			<field name="function">_scheduler_do_reconcile</field>
			<field name="priority">1</field>
		</record>
	</data>
</openerp>